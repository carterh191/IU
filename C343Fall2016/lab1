public class Rain{
  public static void main(String[] args){
    int[] numbers =new int[]{10, 30 ,4, 5, 45, 3, 1, -1, -33};
    int sum = 0;
    for(int i=0; i < numbers.length; i++)
      sum = sum + numbers[i];
    double average = sum / numbers.length;
    System.out.println("Average value of rain:" + average + "inches of rain");
  }
}
